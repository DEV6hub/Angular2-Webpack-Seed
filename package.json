  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV='production' webpack --progress --display-error-details",
    "postinstall": "typings install",
    "server": "webpack-dev-server --inline --progress --display-error-details --port 3000 --open",
    "start": "npm run server",
    "test": "NODE_ENV='test' karma start",
    "test:watch": "NODE_ENV='test' karma start --no-single-run --auto-watch",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "prewebdriver:start": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "e2e": "protractor"
  },
